{
  "name": "Passepartout",
  "manifest_version": 2,
  "version": "0.1",
  "offline_enabled": true,
  "options_page": "options.html",
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content_loadurl.js"  ]
  }],
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "permissions": [
     "tabs",
     "activeTab",
     "alarms",
	"<all_urls>"
  ],
  "background": {
    "scripts": ["jquery.js", "eventPage.js" ],
    "persistent": false
  },
  "settings" : {
    "tabroom" : "surfclub"
  }
}
